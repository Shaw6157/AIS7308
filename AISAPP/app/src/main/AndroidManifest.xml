<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nz.co.ais.aisapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ais"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_ais"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ITActivity" />
        <activity android:name=".TourismActivity" />
        <activity android:name=".HospitalityActivity" />
        <activity android:name=".BusinessActivity"></activity>
    </application>

</manifest>